<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<options>
			<ROOT>
				<TCEforms>
					<sheetTitle>Chart</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<chart.doughnut.cutoutPercentage>
						<TCEforms>
							<label>Cutout Percentage</label>
							<displayCond>FIELD:parentRec.CType:=:chart_doughnut</displayCond>
							<config>
								<type>input</type>
								<size>10</size>
								<eval>trim,int</eval>
								<default>0</default>
								<range>
									<lower>0</lower>
									<upper>85</upper>
								</range>
								<slider>
									<step>5</step>
									<width>200</width>
								</slider>
							</config>
						</TCEforms>
					</chart.doughnut.cutoutPercentage>
				</el>
			</ROOT>
		</options>
	</sheets>
</T3DataStructure>
